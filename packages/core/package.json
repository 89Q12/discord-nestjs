  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/config": "^1.0.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.11",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "discord.js": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
